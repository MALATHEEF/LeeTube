{
    "manifest_version": 4,
    "name": "LeeTube",
    "version": "1.0",
    "description": "This Helps to get solution video from Youtube",
    "permissions": [
    "tabs","activeTab"
  ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "yt.png"
    },
    "icons": {
      "128": "yt.png"
    }
  }
  
